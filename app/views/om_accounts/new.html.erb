<% @header="Access Account" %>

<%= error_messages_for :om_account %>

<% form_for(@om_account) do |f| %>
  <p>
    <b>Account Address</b><br />
    <%= f.text_field :omrl %>
  </p>

  <p>
    <b>Access Credential</b><br />
    Password: <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Access" %>
  </p>
<% end %>

<%= link_to 'Back', om_accounts_path %>
