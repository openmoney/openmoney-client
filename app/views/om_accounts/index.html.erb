<h1>Listing om_accounts</h1>

<table>
  <tr>
    <th>Omrl</th>
  </tr>

<% for om_account in @om_accounts %>
  <tr>
    <td><%=h om_account.omrl %></td>
    <td><%= link_to 'Show', om_account %></td>
    <td><%= link_to 'Edit', edit_om_account_path(om_account) %></td>
    <td><%= link_to 'Destroy', om_account, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New om_account', new_om_account_path %>
